{
  "name": "nrs",
  "version": "1.0.0",
  "description": "A tool to easy switch npm/yarn/pnpm registry",
  "type": "module",
  "bin": "nrs",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "husky install",
    "ts": "NODE_ENV=development ts-node --esm src/index.mts",
    "build": "npx swc src -d lib",
    "test": "ava"
  },
  "keywords": [
    "npm",
    "nrm",
    "nrs",
    "registry",
    "npm registry",
    "npm registry manager",
    "npm registry selector"
  ],
  "author": "CaptainOfPhB <captainofphb@gmail.com>",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "commander": "^9.4.1",
    "execa": "5.1.1",
    "ini": "^3.0.1",
    "node-fetch": "^3.3.1",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.18",
    "@tsconfig/node16": "^1.0.3",
    "@types/ini": "^1.3.31",
    "ava": "^5.2.0",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1",
    "ts-node": "^10.9.1"
  }
}