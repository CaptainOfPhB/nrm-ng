{
  "name": "nrg-ng",
  "version": "1.0.0-beta.1",
  "description": "The next generation of NPM registry management",
  "type": "module",
  "bin": "nrm",
  "main": "lib/index.js",
  "scripts": {
    "prepare": "husky install",
    "nrm": "NODE_ENV=development ts-node --esm src/index.mts",
    "build": "npx swc src -d lib",
    "test": "ava"
  },
  "keywords": [
    "npm",
    "nrm",
    "nrs",
    "npm registry",
    "npm registry selector",
    "npm registry management",
    "npm registry next generation"
  ],
  "repository": {
    "url": "https://github.com/CaptainOfPhB/nrm-ng"
  },
  "author": "CaptainOfPhB <captainofphb@gmail.com>",
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "dependencies": {
    "commander": "^9.4.1",
    "execa": "5.1.1",
    "ini": "^3.0.1",
    "node-fetch": "^3.3.1",
    "picocolors": "^1.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.3.18",
    "@tsconfig/node16": "^1.0.3",
    "@types/ini": "^1.3.31",
    "@types/node": "^20.2.1",
    "ava": "^5.2.0",
    "commitizen": "^4.2.5",
    "conventional-changelog-cli": "^2.2.2",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^8.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.4"
  }
}