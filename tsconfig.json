{
  "extends": "ts-node/node16/tsconfig.json",
  "compilerOptions": {
    "target": "es2016",
    "module": "Node16",
    "moduleResolution": "node16",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "src/**/*.mts"
  ]
}